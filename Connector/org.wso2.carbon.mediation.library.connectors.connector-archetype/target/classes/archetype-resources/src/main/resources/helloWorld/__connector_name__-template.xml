#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<template xmlns="http://ws.apache.org/ns/synapse" name="${connector_name.toLowerCase()}-operation">
    <parameter name="generated_param"/>
    <sequence>
        <log level="full">
            <property name="template_param" expression="$func:generated_param" />
        </log>
        <class name="${package}.${connector_name}" />
    </sequence>
</template>
